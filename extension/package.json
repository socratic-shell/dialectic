{
  "name": "dialectic",
  "displayName": "Dialectic Code Reviews",
  "description": "AI-powered code review panel for collaborative development",
  "version": "0.1.0",
  "publisher": "socratic-shell",
  "repository": {
    "type": "git",
    "url": "https://github.com/socratic-shell/dialectic.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dialectic.showReview",
        "title": "Show Review",
        "category": "Dialectic"
      },
      {
        "command": "dialectic.copyReview",
        "title": "Copy Review",
        "category": "Dialectic",
        "icon": "$(copy)"
      },
      {
        "command": "dialectic.logPIDs",
        "title": "Log PIDs",
        "category": "Dialectic"
      },
      {
        "command": "dialectic.showFileDiff",
        "title": "Show File Diff",
        "category": "Dialectic"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dialectic",
          "title": "Dialectic",
          "icon": "$(git-pull-request)"
        }
      ]
    },
    "views": {
      "dialectic": [
        {
          "id": "dialectic.syntheticPR",
          "name": "Pull Request"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "webpack": "webpack --mode production",
    "webpack-dev": "webpack --mode development",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "ts-loader": "^9.4.0"
  },
  "dependencies": {
    "markdown-it": "^14.1.0"
  }
}
